{
  "name": "webpackdemo",
  "version": "1.0.0",
  "description": "Demo de uso webpack 4",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack-external.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:dev-hot": "webpack-dev-server --config ./webpack-dev-server-hot/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:babelv2": "webpack-dev-server --config ./babelv2/webpack.config.js",
    "build:reactv1": "webpack-dev-server --config ./reactv1/webpack.config.js",
    "build:reactv2": "webpack-dev-server --config ./reactv2/webpack.config.js",
    "build:react-url-loader": "webpack-dev-server --config ./react-url-loader/webpack.config.js",
    "build:prepos-css": "webpack-dev-server --config ./prepos-css/webpack.config.js",
    "build:prevent-dup": "webpack --config ./prevent-duplicate/webpack.config.js",
    "build:use-dlls": "webpack --config ./use-dlls/webpack-dll.config.js",
    "build:prevent-dup-fast": "webpack --config ./use-dlls/webpack.config.js",
    "build:dynamic-dll": "webpack --config ./dynamic-import/webpack-dll.config.js",
    "build:dynamic": "webpack --config ./dynamic-import/webpack.config.js"
  },
  "keywords": [
    "ejemplo",
    "webpack",
    "4"
  ],
  "author": "@alonsodan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.3.0",
    "css-loader": "6.7.3",
    "exact": "^0.8.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "less": "3.9.0",
    "less-loader": "11.1.0",
    "mini-css-extract-plugin": "2.7.2",
    "node-sass": "4.12.0",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "4.1.2",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylus": "0.54.5",
    "stylus-loader": "7.1.0",
    "url-loader": "4.1.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "3.7.2"
  }
}
